{
  "name": "node-p1-uploader",
  "version": "1.0.0",
  "description": "Upload p1 data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericjansen1988/node-p1-uploader.git"
  },
  "author": "Eric Jansen",
  "bugs": {
    "url": "https://github.com/ericjansen1988/node-p1-uploader/issues"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --ext=jsx,ts,tsx,js src",
    "lint:fix": "eslint --ext=jsx,ts,tsx,js src --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "start": "ts-node src/index.ts",
    "start:debug": "ts-node --inspect=5858 --debug-brk --ignore false src/index.ts"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "sequelize": "^5.21.6",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/jest": "^24.0.24",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^12.12.36",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.9.0",
    "typescript": "^3.7.3"
  }
}
